def open(path, flags, mode):
    return _syscall(2, path, flags, mode)

def read(fd):
    return _syscall(0, fd)

def chdir(path):
    # TODO: Make this pass the FULL path. We can use the current path to figure out the full path
    return _syscall(80, path)

def getcwd():
    return _syscall(79)

def execve(pathname, argv, envp):
    return _syscall(59, pathname, argv, envp)

def rmdir(pathname):
    return _syscall(84, pathname)

def unlink(pathname):
    return _syscall(87, pathname)