def getenv(key: str) -> str:
    # We don't have a pointer to the environment, so we'll just fake it (like my girlfriend :( )
    return _internal_get_env(key);

def setenv(key: str, value: str) -> None:
    return _internal_set_env(key, value)

def read(path: str) -> str:
    return _syscall(0, path)
