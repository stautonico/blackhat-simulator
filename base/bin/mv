require("stdio")


def main(args):
    if len(args) != 2:
        print("usage: mv <oldpath> <newpath>")
        return 1

    # TODO: Support writing mv /tmp/myfile /tmp/directorytomove
    #       and it will move myfile to /tmp/directorytomove/myfile.
    #       This is something that needs to be supported by the binary,
    #       not the syscall itself.

    result = rename(args[0], args[1])

    if result < 0:
        perror("mv")
        return 1

    return 0