require("unistd")

def main(argv):
    if len(argv) == 0:
        path = getcwd()
    else:
        path = argv[0]

    # TODO: Support errno for directory not found
    # TODO: Support multiple args
    entries = getdents(path)

    for entry in entries:
        print(entry + " ", False)

    print("") # newline

    return 0