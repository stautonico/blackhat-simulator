require("stdio")
require("unistd")
require("fcntl")

def main(args):
    if len(args) != 1:
        print(f"usage: cat <path>")
        return 1

    fd = open(args[0], O_RDONLY)

    if fd < 0:
        perror("Unable to read file")
        return 1

    result = read(fd)

    if result is None:
        perror("Unable to read file")
    else:
        print(result)

    if close(fd) < 0:
        perror("close")

    return 0